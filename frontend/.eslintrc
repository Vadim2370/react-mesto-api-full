{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": [0, { }],
        "no-underscore-dangle": ["error", { "allow": ["_id"], "allowAfterThis": true }],
        "react/react-in-jsx-scope": "off"
}
}
